    table, th, td {
        border: 1px solid black;
    }

    td {
        text-align: center;
    }
</style>
<script src="//code.jquery.com/jquery-1.11.1.min.js"></script>
<!------ Include the above in your HEAD tag ---------->

<div class="container">
    <form method="post" action="add_programOutcomes">
        <table id="myTable" class=" table revision-list">

            <thead>
                <tr>
                    <td>Code</td>
                    <td>P.O Attribute</td>
                    <td>Program Outcomes</td>
                    <td>Core Values Activated</td>
                </tr>
            </thead>
            <tbody>
                <tr>
                    <td class="col-sm-1">
                        <input type="text" name="code_PO" class="form-control" />
