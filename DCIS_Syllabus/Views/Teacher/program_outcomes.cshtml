                    <td class="col-sm-1">
                        <input type="text" name="attribute_PO" class="form-control" />
                    </td>
                    <td class="col-sm-1">
                        <input type="text" name="outcomes_PO" class="form-control" />
                    </td>
                    <td class="col-sm-1">
                        <input type="text" name="scientia" class="form-control" />
                    </td>
                </tr>
            </tbody>
            <tfoot>
        </table>
        <br />
        <br />
        <button type="submit" class="btn btn-default" value="Add Row">Add Row</button>
    </form>
</div>

